      "options": [
        {
          "value": "provider2",
          "label": "I can provide data"
        },
        {
          "value": "consumer2",
          "label": "I want to use data"
        }
      ]
    },
    "industry": {
      "label": "Industry",
      "tag": "select",
      "required": true,
      "options": [
        {
          "value": "automotive",
