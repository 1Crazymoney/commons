{
  "title": "A cool form title",
  "description": "A cool form description",
  "fields": {
    "name": {
      "label": "Your name",
      "placeholder": "i.e. Jelly McJellyfish",
      "type": "text",
      "required": true
    },
    "email": {
      "label": "Your email",
